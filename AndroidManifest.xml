<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="au.edu.rmit.daniel.socialeventplannerv1"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="au.edu.rmit.daniel.socialeventplannerv1.view.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="au.edu.rmit.daniel.socialeventplannerv1.testdrive.EventTester" >
        </activity>
        <activity
            android:name="au.edu.rmit.daniel.socialeventplannerv1.view.NewEventActivity"
            android:windowSoftInputMode="stateAlwaysHidden" >
        </activity>
        <activity
            android:name="au.edu.rmit.daniel.socialeventplannerv1.view.EditEventsActivity"
            android:windowSoftInputMode="stateAlwaysHidden" >
        </activity>
        <activity android:name="au.edu.rmit.daniel.socialeventplannerv1.view.ViewEventsActivity" >
        </activity>
        <activity
            android:name="au.edu.rmit.daniel.socialeventplannerv1.view.MyCalendarActivity"
            android:label="@string/app_name" >
        </activity>
        <activity android:name="au.edu.rmit.daniel.socialeventplannerv1.view.CalendarNewEventActivity" >
        </activity>
    </application>

</manifest>